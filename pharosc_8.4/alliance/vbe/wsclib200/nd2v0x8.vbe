ENTITY nd2v0x8 IS
GENERIC (
  CONSTANT area          : NATURAL := 7680;   
  CONSTANT cin_a         : NATURAL := 48;     
  CONSTANT cin_b         : NATURAL := 48;     
  CONSTANT rdown_a_z     : NATURAL := 710;    
  CONSTANT rdown_b_z     : NATURAL := 710;    
  CONSTANT rup_a_z       : NATURAL := 780;    
  CONSTANT rup_b_z       : NATURAL := 780;    
  CONSTANT tphl_a_z	 : NATURAL := 59;
  CONSTANT tphl_b_z	 : NATURAL := 111;
  CONSTANT tplh_b_z	 : NATURAL := 234;
  CONSTANT tplh_a_z	 : NATURAL := 288;
  CONSTANT transistors   : NATURAL := 16      
);
PORT (
  a	 : in  BIT;
  b	 : in  BIT;
  z	 : out BIT;
  vdd	 : in  BIT;
  vss	 : in  BIT
);
END nd2v0x8;

ARCHITECTURE behaviour_data_flow OF nd2v0x8 IS

BEGIN
  ASSERT ((vdd and not (vss)) = '1')
  REPORT "power supply is missing on nd2v0x8"
  SEVERITY WARNING;
  z <= not ((a and b)) after 900 ps;
END;
