ENTITY iv1_x3 IS
GENERIC (
  CONSTANT area          : NATURAL := 1000;   
  CONSTANT cin_a         : NATURAL := 19;     
  CONSTANT rdown_a_z     : NATURAL := 1140;   
  CONSTANT rup_a_z       : NATURAL := 1290;   
  CONSTANT tphl_a_z	 : NATURAL := 86;
  CONSTANT tplh_a_z	 : NATURAL := 133;
  CONSTANT transistors   : NATURAL := 4       
);
PORT (
  a	 : in  BIT;
  z	 : out BIT;
  vdd	 : in  BIT;
  vss	 : in  BIT
);
END iv1_x3;

ARCHITECTURE behaviour_data_flow OF iv1_x3 IS

BEGIN
  ASSERT ((vdd and not (vss)) = '1')
  REPORT "power supply is missing on iv1_x3"
  SEVERITY WARNING;
  z <= not (a) after 700 ps;
END;
