ARCHITECTURE behaviour_data_flow OF nd3v0x1 IS

BEGIN
  ASSERT ((vdd and not (vss)) = '1')
  REPORT "power supply is missing on nd3v0x1"
  SEVERITY WARNING;
  z <= not (((a and b) and c)) after 118 ps;
END;
