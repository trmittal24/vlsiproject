ENTITY iv1v4x2 IS
GENERIC (
  CONSTANT area          : NATURAL := 2560;
  CONSTANT cin_a         : NATURAL := 11;     
  CONSTANT rdown_a_z     : NATURAL := 2850;   
  CONSTANT rup_a_z       : NATURAL := 1860;   
  CONSTANT tphl_a_z      : NATURAL := 40;     
  CONSTANT tplh_a_z      : NATURAL := 34;     
  CONSTANT transistors   : NATURAL := 3       
);
PORT (
  a	 : in  BIT;
  z	 : out BIT;
  vdd	 : in  BIT;
  vss	 : in  BIT
);
END iv1v4x2;

ARCHITECTURE behaviour_data_flow OF iv1v4x2 IS

BEGIN
  ASSERT ((vdd and not (vss)) = '1')
  REPORT "power supply is missing on iv1v4x2"
  SEVERITY WARNING;
  z <= not (a) after 96 ps;
END;
