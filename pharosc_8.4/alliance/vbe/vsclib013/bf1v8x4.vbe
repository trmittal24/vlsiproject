ENTITY bf1v8x4 IS
GENERIC (
  CONSTANT area          : NATURAL := 2880;   
  CONSTANT cin_a         : NATURAL := 4;      
  CONSTANT rdown_a_z     : NATURAL := 840;    
  CONSTANT rup_a_z       : NATURAL := 1070;   
  CONSTANT tpll_a_z      : NATURAL := 104;    
  CONSTANT tphh_a_z      : NATURAL := 86;     
  CONSTANT transistors   : NATURAL := 6       
);
PORT (
  a	 : in  BIT;
  z	 : out BIT;
  vdd	 : in  BIT;
  vss	 : in  BIT
);
END bf1v8x4;

ARCHITECTURE behaviour_data_flow OF bf1v8x4 IS

BEGIN
  ASSERT ((vdd and not (vss)) = '1')
  REPORT "power supply is missing on bf1v8x4"
  SEVERITY WARNING;
  z <= a after 119 ps;
END;
