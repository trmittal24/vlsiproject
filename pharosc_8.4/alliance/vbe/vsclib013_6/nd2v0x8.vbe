ENTITY nd2v0x8 IS
GENERIC (
  CONSTANT area          : NATURAL := 6912;   
  CONSTANT cin_a         : NATURAL := 27;     
  CONSTANT cin_b         : NATURAL := 26;     
  CONSTANT rdown_a_z     : NATURAL := 460;    
  CONSTANT rdown_b_z     : NATURAL := 460;    
  CONSTANT rup_a_z       : NATURAL := 620;    
  CONSTANT rup_b_z       : NATURAL := 620;    
  CONSTANT tphl_a_z      : NATURAL := 32;     
  CONSTANT tphl_b_z      : NATURAL := 33;     
  CONSTANT tplh_b_z      : NATURAL := 43;     
  CONSTANT tplh_a_z      : NATURAL := 49;     
  CONSTANT transistors   : NATURAL := 16      
);
PORT (
  a	 : in  BIT;
  b	 : in  BIT;
  z	 : out BIT;
  vdd	 : in  BIT;
  vss	 : in  BIT
);
END nd2v0x8;

ARCHITECTURE behaviour_data_flow OF nd2v0x8 IS

BEGIN
  ASSERT ((vdd and not (vss)) = '1')
  REPORT "power supply is missing on nd2v0x8"
  SEVERITY WARNING;
  z <= not ((a and b)) after 53 ps;
END;
