ENTITY or2v4x1 IS
GENERIC (
  CONSTANT area          : NATURAL := 2880;   
  CONSTANT cin_a         : NATURAL := 9;      
  CONSTANT cin_b         : NATURAL := 9;      
  CONSTANT rdown_a_z     : NATURAL := 2680;   
  CONSTANT rdown_b_z     : NATURAL := 2680;   
  CONSTANT rup_a_z       : NATURAL := 3300;   
  CONSTANT rup_b_z       : NATURAL := 3300;   
  CONSTANT tpll_a_z      : NATURAL := 143;    
  CONSTANT tphh_b_z      : NATURAL := 70;     
  CONSTANT tpll_b_z      : NATURAL := 134;    
  CONSTANT tphh_a_z      : NATURAL := 76;     
  CONSTANT transistors   : NATURAL := 6       
);
PORT (
  a	 : in  BIT;
  b	 : in  BIT;
  z	 : out BIT;
  vdd	 : in  BIT;
  vss	 : in  BIT
);
END or2v4x1;

ARCHITECTURE behaviour_data_flow OF or2v4x1 IS

BEGIN
  ASSERT ((vdd and not (vss)) = '1')
  REPORT "power supply is missing on or2v4x1"
  SEVERITY WARNING;
  z <= (a or b) after 181 ps;
END;
