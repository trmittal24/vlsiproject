ENTITY nd2av0x6 IS
GENERIC (
  CONSTANT area          : NATURAL := 6912;   
  CONSTANT cin_b         : NATURAL := 22;     
  CONSTANT cin_a         : NATURAL := 30;     
  CONSTANT rdown_b_z     : NATURAL := 950;    
  CONSTANT rdown_a_z     : NATURAL := 950;    
  CONSTANT rup_b_z       : NATURAL := 1030;   
  CONSTANT rup_a_z       : NATURAL := 1030;   
  CONSTANT tphl_b_z      : NATURAL := 111;    
  CONSTANT tplh_b_z      : NATURAL := 234;    
  CONSTANT tpll_a_z      : NATURAL := 318;    
  CONSTANT tphh_a_z      : NATURAL := 340;    
  CONSTANT transistors   : NATURAL := 15      
);
PORT (
  b	 : in  BIT;
  a	 : in  BIT;
  z	 : out BIT;
  vdd	 : in  BIT;
  vss	 : in  BIT
);
END nd2av0x6;

ARCHITECTURE behaviour_data_flow OF nd2av0x6 IS

BEGIN
  ASSERT ((vdd and not (vss)) = '1')
  REPORT "power supply is missing on nd2av0x6"
  SEVERITY WARNING;
  z <= (not (b) or a) after 900 ps;
END;
