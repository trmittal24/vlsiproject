  CONSTANT cin_c         : NATURAL := 20;     
  CONSTANT rdown_a_z     : NATURAL := 2380;   
  CONSTANT rdown_b_z     : NATURAL := 2380;   
  CONSTANT rdown_c_z     : NATURAL := 2380;   
  CONSTANT rup_a_z       : NATURAL := 2380;   
