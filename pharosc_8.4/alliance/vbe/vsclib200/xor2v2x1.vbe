ENTITY xor2v2x1 IS
GENERIC (
  CONSTANT area          : NATURAL := 5184;   
  CONSTANT cin_b         : NATURAL := 11;     
  CONSTANT cin_a         : NATURAL := 11;     
  CONSTANT rdown_b_z     : NATURAL := 4070;   
  CONSTANT rdown_a_z     : NATURAL := 4070;   
  CONSTANT rup_b_z       : NATURAL := 4590;   
  CONSTANT rup_a_z       : NATURAL := 4590;   
  CONSTANT tplh_a_z	 : NATURAL := 261;
  CONSTANT tphl_b_z	 : NATURAL := 292;
  CONSTANT tplh_b_z	 : NATURAL := 293;
  CONSTANT tphh_b_z	 : NATURAL := 366;
  CONSTANT tphl_a_z	 : NATURAL := 377;
  CONSTANT tpll_a_z	 : NATURAL := 388;
  CONSTANT tpll_b_z	 : NATURAL := 389;
  CONSTANT tphh_a_z	 : NATURAL := 405;
  CONSTANT transistors   : NATURAL := 10      
);
PORT (
  b	 : in  BIT;
  a	 : in  BIT;
  z	 : out BIT;
  vdd	 : in  BIT;
  vss	 : in  BIT
);
END xor2v2x1;

ARCHITECTURE behaviour_data_flow OF xor2v2x1 IS

BEGIN
  ASSERT ((vdd and not (vss)) = '1')
  REPORT "power supply is missing on xor2v2x1"
  SEVERITY WARNING;
  z <= (b xor a) after 1000 ps;
END;
