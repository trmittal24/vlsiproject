ENTITY nr2v1x3 IS
GENERIC (
  CONSTANT area          : NATURAL := 4608;   
  CONSTANT cin_a         : NATURAL := 30;     
  CONSTANT cin_b         : NATURAL := 30;     
  CONSTANT rdown_a_z     : NATURAL := 910;    
  CONSTANT rdown_b_z     : NATURAL := 910;    
  CONSTANT rup_a_z       : NATURAL := 1530;   
  CONSTANT rup_b_z       : NATURAL := 1530;   
  CONSTANT tplh_a_z	 : NATURAL := 121;
  CONSTANT tplh_b_z	 : NATURAL := 161;
  CONSTANT tphl_b_z	 : NATURAL := 193;
  CONSTANT tphl_a_z	 : NATURAL := 298;
  CONSTANT transistors   : NATURAL := 10      
);
PORT (
  a	 : in  BIT;
  b	 : in  BIT;
  z	 : out BIT;
  vdd	 : in  BIT;
  vss	 : in  BIT
);
END nr2v1x3;

ARCHITECTURE behaviour_data_flow OF nr2v1x3 IS

BEGIN
  ASSERT ((vdd and not (vss)) = '1')
  REPORT "power supply is missing on nr2v1x3"
  SEVERITY WARNING;
  z <= not ((a or b)) after 900 ps;
END;
