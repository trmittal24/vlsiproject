ARCHITECTURE behaviour_data_flow OF xor2v0x3 IS

BEGIN
  ASSERT ((vdd and not (vss)) = '1')
  REPORT "power supply is missing on xor2v0x3"
  SEVERITY WARNING;
  z <= (b xor a) after 99 ps;
END;
