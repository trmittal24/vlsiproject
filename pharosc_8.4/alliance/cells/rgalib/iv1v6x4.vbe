ENTITY iv1v6x4 IS
GENERIC (
  CONSTANT area          : NATURAL := 2816;   
  CONSTANT cin_a         : NATURAL := 11;     
  CONSTANT rdown_a_z     : NATURAL := 580;    
  CONSTANT rup_a_z       : NATURAL := 1060;   
  CONSTANT tphl_a_z      : NATURAL := 30;     
  CONSTANT tplh_a_z      : NATURAL := 40;     
  CONSTANT transistors   : NATURAL := 4       
);
PORT (
  a	 : in  BIT;
  z	 : out BIT;
  vdd	 : in  BIT;
  vss	 : in  BIT
);
END iv1v6x4;

ARCHITECTURE behaviour_data_flow OF iv1v6x4 IS

BEGIN
  ASSERT ((vdd and not (vss)) = '1')
  REPORT "power supply is missing on iv1v6x4"
  SEVERITY WARNING;
  z <= not (a) after 56 ps;
END;
