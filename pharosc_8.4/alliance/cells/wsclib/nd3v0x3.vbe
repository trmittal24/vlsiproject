ENTITY nd3v0x3 IS
GENERIC (
  CONSTANT area          : NATURAL := 5760;
  CONSTANT cin_a         : NATURAL := 10;     
  CONSTANT cin_b         : NATURAL := 10;     
  CONSTANT cin_c         : NATURAL := 9;      
  CONSTANT rdown_a_z     : NATURAL := 1280;   
  CONSTANT rdown_b_z     : NATURAL := 1270;   
  CONSTANT rdown_c_z     : NATURAL := 1280;   
  CONSTANT rup_a_z       : NATURAL := 1490;   
  CONSTANT rup_b_z       : NATURAL := 1480;   
  CONSTANT rup_c_z       : NATURAL := 1480;   
  CONSTANT tphl_a_z      : NATURAL := 42;     
  CONSTANT tphl_b_z      : NATURAL := 41;     
  CONSTANT tphl_c_z      : NATURAL := 37;     
  CONSTANT tplh_c_z      : NATURAL := 48;     
  CONSTANT tplh_b_z      : NATURAL := 56;     
