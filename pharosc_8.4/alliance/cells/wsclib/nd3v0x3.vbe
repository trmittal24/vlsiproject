ARCHITECTURE behaviour_data_flow OF nd3v0x3 IS

BEGIN
  ASSERT ((vdd and not (vss)) = '1')
  REPORT "power supply is missing on nd3v0x3"
  SEVERITY WARNING;
  z <= not (((a and b) and c)) after 82 ps;
END;
